#!${bamboo_build_working_directory}/python-env-centos/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'jira==2.0.0','console_scripts','jirashell'
__requires__ = 'jira==2.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('jira==2.0.0', 'console_scripts', 'jirashell')()
    )
